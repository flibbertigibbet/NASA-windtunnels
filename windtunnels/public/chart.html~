<html>
  <head>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      google.load("visualization", "1", {packages:["corechart"]});

      function drawVisualization() {
      var queryText = encodeURIComponent("SELECT Hours, Year FROM 1T8zOrOm79lND_Ebc-dKHa1-94DWu95BLw71CRCo");

      google.visualization.drawChart({
        "containerId": "visualization_div",
        "dataSourceUrl": 'http://www.google.com/fusiontables/gvizdata?tq=',
        "query":"SELECT Hours, Year FROM 1T8zOrOm79lND_Ebc-dKHa1-94DWu95BLw71CRCo",
        "refreshInterval": 60,
        "chartType": "BarChart",
        "options": {
          "title":"Yearly User Hours of NASA Wind Tunnels",
          "hAxis": {"title": "Year"},
          "vAxis": {"title": "User Hours"}
        }
      });
    }

    google.setOnLoadCallback(drawVisualization);
  </script>
</head>
<body style="font-family: Arial;border: 0 none;">
